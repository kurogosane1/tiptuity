
<div class="jumbotron">
    <div class='row'>
        <div class='col-sm-6'>
            {{!--this is where the logo would go--}}
        </div>
        <div class='col-sm-6'>
            {{sumTips}}
        </div>
    </div>

<div class='container-fuild'>

    <div class="col-sm-3 col-lg-2">
        <nav class="navbar navbar-default navbar-fixed-side">
            <!-- normal collapsible navbar markup -->
            <ul>
                <li><a href="">Home</a></li>
                <li><a href="">Alex</a></li>
                <li><a href="">Syed</a></li>
                <li><a href="">Faisal</a></li>
            </ul>

        </nav>
    </div>
    <div class="col-sm-6 col-lg-10">
        <!-- your page content -->
        <div id='employee-list'>
            {{#each tips}}

            <div id='table'>
                <table class='table'>
                    <tr>
                        {{!--//this is where the employee img would go--}}
                        <th><img src="#" alt=""></th>
                        <th>{{this.first_name}}</th>
                        <th>{{this.last_name}}</th>
                    </tr>
                    <tr>
                        <td>{{this.tip_amount}}</td>
                        <td>{{this.tip_fees}}</td>
                        <td>{{this.tip_final}}</td>
                    </tr>
                </table>

            </div>

            {{/each}}

        </div>

    </div>

</div>
</div>